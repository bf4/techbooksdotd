<?xml version="1.0" encoding="UTF-8"?>
<Module>
  <ModulePrefs title="Tech Books Deals of the Day"
    title_url="http://techbooksdotd.heroku.com"
    height="500"
    width="250"
    description="Shows the deals of the day for leading technology book publishers."
    category="technology"
    screenshot="http://techbooksdotd.heroku.com/gadget/tbdotd_screenshot.png"
    thumbnail="http://techbooksdotd.heroku.com/gadget/tbdotd_thumbnail.png"
    directory_title="Tech Books Deals of the Day"
    author="Rob Warner"
    author_email="hoop33+gadgets@gmail.com"
    author_location="Jacksonville, FL, US"
    author_affiliation="Grailbox"
    author_photo="http://grailbox.com/images/rwarner.jpg"
    author_aboutme="Sr. Technical Staff Member at Availity, LLC"
    author_link="http://grailbox.com">
    <Icon>http://techbooksdotd.heroku.com/gadget/book_32.png</Icon>
    <Require feature="dynamic-height" />
    <Require feature="setprefs" />
  </ModulePrefs>

  <Content type="html" view="default,home,profile,preview">
    <![CDATA[
    <link href="http://techbooksdotd.heroku.com/gadget/tbdotd.css" type="text/css" rel="stylesheet" />
    <script type="text/javascript">
      document.body.className += ' tbdotd';
    </script>
    <div id="main">
      <div id="apress" class="vendor">
        <a id="apress_img_link" href="" target="_blank">
          <img id="apress_img" class="figure" alt="Apress Book" src="" width="125" height="164" />
        </a>
        <a id="apress_link" class="caption" href="http://apress.com" target="_blank">Apress</a>
      </div>
      <div id="oreilly" class="vendor">
        <a id="oreilly_img_link" href="" target="_blank">
          <img id="oreilly_img" class="figure" alt="O'Reilly Book" src="" width="125" height="164" />
        </a>
        <a id="oreilly_link" class="caption" href="http://oreilly.com" target="_blank">O'Reilly</a>
      </div>
      <div id="manning" class="vendor">
        <p class="figure">Manning</p>
        <p id="manning_link" class="caption">Manning</p>
      </div>
      <div id="credit">
        <a target="_blank" href="http://techbooksdotd.heroku.com">Tech Books Deals of the Day</a>
      </div>
    </div>
    <script src="http://techbooksdotd.heroku.com/gadget/jsonmin.js" type="text/javascript"></script>
    <script src="http://techbooksdotd.heroku.com/gadget/crossPlatform.js" type="text/javascript"></script>
    <script src="http://techbooksdotd.heroku.com/gadget/tbdotd.js" type="text/javascript"></script>
    ]]>
  </Content>
  <Content type="url" view="canvas" href="http://techbooksdotd.heroku.com" />
</Module>
